{
  "name": "thnkandgrow-antigravity",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack -p 9999",
    "build": "next build",
    "build:standalone": "next build && npm run build:static",
    "build:static": "next build --experimental-generate-static",
    "start": "next start -p 9999",
    "start:standalone": "node .next/standalone/server.js",
    "lint": "next lint",
    "cache:download": "ts-node scripts/cache-graphql-data.ts",
    "build:cached": "npm run cache:download && npm run build",
    "warm-cache": "node scripts/warm-cache.mjs",
    "build:warm": "npm run build && npm run warm-cache"
  },
  "dependencies": {
    "@apollo/client": "^3.11.11",
    "graphql": "^16.10.0",
    "next": "16.0.3",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "dotenv": "^17.2.3",
    "eslint": "^9",
    "eslint-config-next": "16.0.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  }
}
